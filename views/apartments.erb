<div class="row spacer">
	<row>
		<div class="col-md-9">
			<div id="apartMap"></div>
		</div>

		<div class="col-md-3">
			<div class="panel panel-default spacer">
				<div class="panel-heading">
					Show apartments
				</div>
				<div class="panel-body">
					<div class="radio">
						<label>
							<input type="radio" name="apartmentCostBand" id="costBandAll" value="ALL" checked>
							All</label>
					</div>
					<div class="radio">
						<label>
							<input type="radio" name="apartmentCostBand" id="costBand1" value="Band1">
							rent less than $500/month</label>
					</div>
					<div class="radio">
						<label>
							<input type="radio" name="apartmentCostBand" id="costBand2" value="Band2">
							rent $500-999/month</label>
					</div>
					<div class="radio">
						<label>
							<input type="radio" name="apartmentCostBand" id="costBand3" value="Band3">
							rent $1000-1250/month</label>
					</div>
				</div>
			</div>
			<div class="panel panel-default">
				<div class="panel-heading">
					Other features
				</div>
				<div class="panel-body">
					<div class="radio">
						<label>
							<input type="checkbox" name="viewOptions" id="viewStations" value="Stations">
							Accessible Stations</label>
					</div>
					<div class="radio">
						<label>
							<input type="checkbox" name="viewOptions" id="viewBusinesses" value="Businesses">
							Wheelchair Friendly Businesses</label>
					</div>
				</div>
			</div>

		</div><!-- end of upper right column -->
	</row>
</div><!-- end of top content row containing filters and map -->

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
<script src='https://api.tiles.mapbox.com/mapbox.js/v1.6.2/mapbox.js'></script>
<script src='//cdnjs.cloudflare.com/ajax/libs/moment.js/2.6.0/moment.min.js'></script>
<script src="js/unlock-philadelphia-apart.js"></script>
